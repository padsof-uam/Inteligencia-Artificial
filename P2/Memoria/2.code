;; Devuelve el valor de la heurÃ­stica en el planeta actual.
;; 
;; IN: 	state: planeta actual
;; 		sensors: lista de tuplas planeta-heurÃ­stica
;; OUT: valor de la heurÃ­stica en el planeta actual
;;
;; pseudocode

(defun f-h-galaxy (state sensors)
  (if (null sensors)
      nil
	  (let ((sensor (first sensors)))
	    (if (equal state (car sensor))
	        (cadr sensor)
	        (f-h-galaxy state (rest sensors))))))

;; Examples

(f-h-galaxy 'Sirtis *sensors*) ;-> 0
(f-h-galaxy 'Avalon *sensors*) ;-> 5

;; end


